{
  "expo": {
    "name": "promotions-app",
    "slug": "promotions-app",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.promotionsapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.promotionsapp",
      "permissions": [
        "android.permission.DETECT_SCREEN_CAPTURE"
      ]
    }
  }
}